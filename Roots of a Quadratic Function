a = int(input("Enter a number for a: "))
b = int(input("Enter a number for b: "))
c = int(input("Enter a number for c: "))

d = b * b -4 * a * c
if d == 0:
  print("There is one real root")
  print(d)
elif d < 0:
  print("There are no real roots")
  print(d)
else:
  print("There are two real roots")
  print(d)
